\newcommand\figbase[3]{
	\begin{figure}[!ht]
		\centering
		#1
		\caption{#3}
		\label{fig:#2}
	\end{figure}
}


\newcommand\wrapfigbase[3]{
	\begin{wrapfigure}{r}{0.5\textwidth}
		\centering
		#1
		\caption{#3}
		\label{fig:#2}
	\end{wrapfigure}
}

% #1 Filename
% #2 Figure width
% #3 Caption
\newcommand\fig[3]{
	\figbase{\includegraphics[width=#2\linewidth]{\dirfigs#1}}{#1}{#3}
}
% #1 Filename
% #2 Figure width
% #3 Caption
\newcommand\wfig[3]{
	\figbase{\includegraphics[width=#2\linewidth]{\dirfigs#1}}{#1}{#3}
}
% #1 .tex file and label
% #2 Caption
\newcommand\schem[2]{
	\begin{figure}
		\centering
		\input{\dirfigs#1}
		\caption{#2}
		\label{sch:#1}
	\end{figure}
}
